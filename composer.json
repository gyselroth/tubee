{
    "name": "gyselroth/tubee",
    "description": "Tubee",
    "keywords": ["import", "export", "data-management"],
    "homepage": "https://github.com/gyselroth/tubee",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Raffael Sahli",
            "email": "sahli@gyselroth.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tubee\\": "src/lib",
            "Tubee\\Testsuite\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/raffis/php-mongomock"
        },
        {
            "type": "git",
            "url": "https://github.com/raffis/smb"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2",
        "psr/log": "^1.0.0",
        "ext-mongodb": "*",
        "ext-curl": "*",
        "ext-apcu": "*",
        "ext-json":  "*",
        "gyselroth/micro-auth": "^1.1.0",
        "gyselroth/micro-container": "^2.3.3",
        "psr/http-factory": "^1.0.0",
        "psr/container": "^1.0.0",
        "mongodb/mongodb": "1.4.*",
        "ulrichsg/getopt-php": "^3.0",
        "zendframework/zend-mail": "2.9.*",
        "gyselroth/mongodb-php-task-scheduler": "4.0.11",
        "hassankhan/config": "^2.0.0",
        "symfony/yaml": "~3.1",
        "monolog/monolog": "^2.0.0-beta1",
        "bramus/monolog-colored-line-formatter": "~2.0",
        "zendframework/zend-servicemanager": "^3.3",
        "icewind/smb": "dev-truncate-fix",
        "psr/cache": "^1.0",
        "dreamscapes/ldap-core": "^3.1",
        "guzzlehttp/psr7": "^1.4",
        "mindplay/middleman": "^3.0",
        "lcobucci/content-negotiation-middleware": "^1.0",
        "zendframework/zend-diactoros": "^1.7",
        "psr/http-server-middleware": "*",
        "middlewares/utils": "^1.0",
        "middlewares/trailing-slash": "^1.0",
        "middlewares/access-log": "^1.0",
        "middlewares/payload": "^2.0",
        "middlewares/negotiation": "^1.0",
        "nikic/fast-route": "^1.3",
        "middlewares/fast-route": "^1.0",
        "php-jsonpatch/php-jsonpatch": "^3.0",
        "stolt/json-merge-patch": "^1.0",
        "psr/http-message": "^1.0",
        "gyselroth/stream-iterator": "^1.0.2",
        "guzzlehttp/guzzle": "~6.0",
        "paragonie/halite": "^4",
        "vanilla/garden-schema": "^2.2",
        "cache/void-adapter": "^1.0",
        "cache/apcu-adapter": "^1.0",
        "psr/simple-cache": "^1.0",
        "helmich/mongomock": "dev-master#7bb0b5d",
        "zendframework/zend-filter": "^2.9",
        "league/event": "^2.2",
        "composer/package-versions-deprecated": "^1.11"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "suggest": {
        "ext-v8js": "*",
        "ext-ldap": "*",
        "ext-mysqli": "*",
        "ext-pdo": "*",
        "ext-imagick": "*",
        "ext-xml": "*",
        "ext-sqlsrv":  "*"
    },
    "require-dev": {
        "phpv8/v8js-stubs": "^1.4.1",
        "friendsofphp/php-cs-fixer": "^2.16.1",
        "phpunit/phpunit": "^5.7.0",
        "phpstan/phpstan": "^0.12"
    }
}
